//- Despite this being the extractor file, all the content corresponds to the factory item. -//
extractor_menu#extractor_menu(style="display:none")
    #factory_header header
    #factory_items 
        |items
        br
        itemList#itemList
        
        
       
    #factory_actions actions
        button#combine(onclick='combineItems()') Combine items
        #testItems
        #errorItems
        button#hideMenu(onclick="hide('extractor_menu','fadeOutDown', 1)") Close Menu
    #energy_cores cores
    #factory_progress 
        button#updateCombinations(onclick='retrieveCombinationTimes()') update Combinations
        #progress_bars